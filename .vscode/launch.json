  "version": "0.2.0",
  "configurations": [
    {
      "name": "SugarPros Dev",
      "request": "launch",
      "type": "dart",
      "args": ["--dart-define", "app.env=dev"]
    },
    {
      "name": "SugarPros Staging",
      "request": "launch",
      "type": "dart",
      "args": ["--dart-define", "app.env=staging"]
    },
    {
      "name": "SugarPros Prod",
      "request": "launch",
      "type": "dart",
      "args": ["--dart-define", "app.env=prod"]
    },
    {
      "name": "SugarPros (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "args": ["--dart-define", "app.env=prod"]
    },
    {
      "name": "SugarPros (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "args": ["--dart-define", "app.env=prod"]
    }
  ]
}
